{
    "name": "tecnogo/laravel-meli-sdk",
    "description": "Laravel package for MeliSDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Starck",
            "email": "valentin.starck@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.8",
        "tecnogo/meli-sdk": "dev-master",
        "illuminate/routing": "^5.9",
        "illuminate/events": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Tecnogo\\LaravelMeliSdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tecnogo\\LaravelMeliSdk\\ServiceProvider"
            ],
            "aliases": {
                "MeliSdk": "Tecnogo\\LaravelMeliSdk\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}

{
    "name": "tecnogo/laravel-meli-sdk",
    "description": "Laravel package for MeliSDK",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Valentin Starck",
            "email": "valentin.starck@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.8",
        "illuminate/routing": "^5.8",
        "illuminate/events": "^5.8",
        "tecnogo/meli-sdk": "1.0.1",
        "illuminate/console": "^5.8"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/juanicastellan0/meli-sdk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tecnogo\\LaravelMeliSdk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tecnogo\\LaravelMeliSdk\\ServiceProvider"
            ],
            "aliases": {
                "MeliSdk": "Tecnogo\\LaravelMeliSdk\\Facade"
            }
        }
    }
}
